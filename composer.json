{
    "name": "dvikan/simple-parts",
    "description": "Simple components for building applications",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "dvikan\\SimpleParts\\": [
              "src",
              "src/cache",
              "src/common",
              "src/config/",
              "src/console",
              "src/container",
              "src/error-handler",
              "src/file",
              "src/http",
              "src/json",
              "src/logger",
              "src/migrator",
              "src/renderer/",
              "src/router/",
              "src/session",
              "src/shell"
            ]
        },
        "files": [
            "src/renderer/functions.php"
        ]
    },
    "autoload-dev": {
      "psr-4": {
        "dvikan\\SimpleParts\\": "test"
      }
    },
    "require-dev": {
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "ext-json": "*",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-imap": "*",
        "ext-pdo": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*"
    }
}
