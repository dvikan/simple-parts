{
    "name": "dvikan/simple-parts",
    "description": "Simple components for building applications",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "dvikan\\SimpleParts\\": [
              "tests/",
              "src/",
              "src/logger/",
              "src/json/",
              "src/http/",
              "src/cache/"
            ]
        },
        "files": [
            "src/utils.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
      "ext-json": "*",
        "ext-curl": "*",
      "ext-simplexml": "*",
      "ext-imap": "*",
      "ext-pdo": "*",
      "ext-libxml": "*"
    }
}
